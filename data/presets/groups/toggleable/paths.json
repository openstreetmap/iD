{
    "name": "Paths",
    "description": "Sidewalks, Foot Paths, Cycle Paths, etc.",
    "toggleable": true,
    "matches": {
        "geometry": ["line", "point"],
        "anyTags": {
            "highway": {
                "path": true,
                "footway": true,
                "cycleway": true,
                "bridleway": true,
                "pedestrian": true,
                "corridor": true,
                "steps": true,
                "service": true,
                "track": true,
                "bus_stop": true
            },
            "routing:entrance": {
                "main": true
            }

        }
    }
}
