[
    {
        "title": "Highway",
        "name": "highway",
        "match": {
            "type": ["line"],
            "tags": {
                "highway": "*"
            }
        },
        "form": [
            {
                "tag": "highway",
                "title": "Highway Type",
                "type": "select",
                "option": ["primary", "secondary", "tertiary"]
            }
        ]
    },
    {
        "title": "Cafe",
        "name": "cafe",
        "match": {
            "type": ["node", "area"],
            "tags": {
                "amenity": "cafe"
            }
        },
        "form": [
            {
                "tag": "phone",
                "type": "tel"
            },
            {
                "tag": "fax",
                "type": "tel"
            },
            {
                "tag": "website",
                "type": "url"
            },
            {
                "tag": "email",
                "type": "email"
            },
            {
                "tag": "internet_access",
                "title": "Internet Access",
                "type": "select",
                "option": ["yes", "wlan","wired","terminal","no"]
            },
            {
                "tag": "internet_access:fee",
                "title": "Internet Access Fee",
                "type": "select",
                "option": ["yes", "no"]
            }
        ]
    }
]
